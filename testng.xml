<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Parallel Tests" parallel="methods" thread-count="2" data-provider-thread-count="2">
	<parameter name="browser" value="chrome" />
	<listeners>
		<listener class-name="utilities.TestEvents" />
		<listener class-name="org.uncommons.reportng.HTMLReporter" />
		<listener class-name="utilities.TestAnnotationTransformer" />
	</listeners>
	<test name="Flights">
		<classes>
			<class name="com.goibibo.Flights" />
		</classes>
	</test>
</suite> <!-- Suite -->
